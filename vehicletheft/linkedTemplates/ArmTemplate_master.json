{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vehicletheft"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"VehicleTheftFileServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'VehicleTheftFileServer'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vehicletheft1.dfs.core.windows.net/"
		},
		"VehicleTheftFileServer_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\ADFDATA"
		},
		"VehicleTheftFileServer_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "cheng\\lchen"
		},
		"vehicleTheftintegrationRuntime_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/68151ce9-0181-4d75-96b9-6b9609554994/resourcegroups/azureadfpiplines/providers/Microsoft.DataFactory/factories/akilaazureadf/integrationruntimes/sqlir"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vehicletheft_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"VehicleTheftFileServer_password": {
						"value": "[parameters('VehicleTheftFileServer_password')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"vehicleTheftintegrationRuntime_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('vehicleTheftintegrationRuntime_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"VehicleTheftFileServer_properties_typeProperties_host": {
						"value": "[parameters('VehicleTheftFileServer_properties_typeProperties_host')]"
					},
					"VehicleTheftFileServer_properties_typeProperties_userId": {
						"value": "[parameters('VehicleTheftFileServer_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}